//! Represents a linked list with pure functional constructs, that have two
//! variants:
//!
//! - [`Cons`]: a head of the list
//! - [`Nil`]: the end of the list
@stdlib
public data List (^a) {
  Cons : a -> List a -> List a,
  Nil  : List a
}

@stdlib
public data Maybe (^a) {
  Just(a),
  Nothing
}

@stdlib
@internal
public data String

type StringList = List String

public
head : ^a. a -> Maybe a
head (Cons x _) = Just x
head Nil        = Nothing

public
println : String -> ()

