//! Represents a linked list with pure functional constructs, that have two
//! variants:
//!
//! - [`Cons`]: a head of the list
//! - [`Nil`]: the end of the list
@stdlib
public data List (a) {
  Cons(a, List a),
  Nil;
}

@stdlib
public data Maybe (a) {
  Just(a),
  Nothing;
}

public
head : [a] => List a -> Maybe a
head (Cons x _) = Just x
head Nil        = Nothing

Pao : Pao { Pao }
